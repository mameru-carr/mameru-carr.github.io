{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Quotes",
  "description": "A collection of quotes that I like",
  "properties": {
    "quotes": {
      "type": "array",
      "description": "An array of quotes with their authors if known",
      "properties": {
        "quote": {
          "type": "string"
        },
        "author": {
          "type": "string"
        }
      },
      "required": ["quote"]
    }
  }
}